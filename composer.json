{
    "name": "wolftotem4/imagestorage",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "intervention/image": "^2.4",
        "illuminate/http": "5.5",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/filesystem": "5.5",
        "guzzlehttp/guzzle": "~6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "WTotem\\ImageStorage\\": "src/ImageStorage/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WTotem\\ImageStorage\\ImageStorageProvider"
            ],
            "aliases": {
                "ImageStorage": "WTotem\\ImageStorage\\Facades\\ImageStorage"
            }
        }
    }
}
